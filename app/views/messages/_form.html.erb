
<div class="card">
  <h5 class="card-header">New Message</h5>
  <div class="card-body">
    <%= simple_form_for @message do |f| %>
        <%= f.input :title, label: "Message Title" %>
        <%= f.input :description %><br>
        <%= f.button :submit, class:'btn btn-dark' %>
   <% end %>
  </div>
</div>









